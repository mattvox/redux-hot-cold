{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "arrow-body-style": ["off", "always"],
        "arrow-parens": ["error", "always"],
        "func-names": ["error", "never"],
        "jsx-quotes": ["error", "prefer-single"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    }
}
